<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.yuezhikong.GUITest.MainGUI.Controller">
  <MenuBar VBox.vgrow="NEVER">
    <Menu mnemonicParsing="false" text="帮助">
      <MenuItem mnemonicParsing="false" onAction="#AboutThisProgram" text="关于此程序" />
    </Menu>
  </MenuBar>
  <AnchorPane VBox.vgrow="ALWAYS">
    <Label alignment="CENTER" layoutX="208.0" layoutY="86.0" prefHeight="19.0" prefWidth="187.0" style="&#10;" text="欢迎使用JavaIM" textAlignment="CENTER" textFill="#000000">
      <font>
        <Font size="18.0" />
      </font>
    </Label>
    <Button fx:id="ServerMode" layoutX="124.0" layoutY="234.0" mnemonicParsing="false" onAction="#OnUserClickStartServer" text="服务端模式" />
    <Label layoutX="200.0" layoutY="161.0" text="请选择使用客户端模式还是服务端模式" />
    <Button fx:id="ClientMode" layoutX="395.0" layoutY="234.0" mnemonicParsing="false" onAction="#OnUserClickStartClient" text="客户端模式" />
  </AnchorPane>
